{
  "name": "markaspot/markaspot_map",
  "type": "drupal-module",
  "description": "Mark-a-Spot map visualisation",
  "homepage": "https://github.com/markaspot/markaspot_map",
  "authors": [
    {
      "name": "Holger Kreis (markaspot)",
      "homepage": "https://www.drupal.org/u/markaspot",
      "role": "Maintainer"
    }
  ],
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "support": {
    "issues": "http://drupal.org/project/issues/markaspot_map",
    "source": "https://github.com/markaspot/markaspot_map"
  },
  "require": {
    "composer/installers": "~1.0",
    "drmonty/leaflet": "1.0.0-rc.3",
    "lvoogdt/Leaflet.awesome-markers": "2.0.2",
    "Leaflet/Leaflet.fullscreen": "1.0.1",
    "CliffCloud/Leaflet.EasyButton": "1.3.1",
    "imakewebthings/waypoints": "4.0.0",
    "socib/Leaflet.TimeDimension": "1.0.6",
    "nezasa/iso8601-js-period": "0.2.0.0",
    "StevenLevithan/dateFormat": "1.2.3",
    "Leaflet/Leaflet.heat": "0.2.0",
    "Leaflet/Leaflet.markercluster": "1.0.0",
    "FortAwesome/Font-Awesome": "4.6.3",
    "drupal/search_api": "1.x-dev",
    "drupal/views_infinite_scroll": "1.x-dev",
    "drupal/better_exposed_filters": "dev-3.x",
    "markaspot/service_request": "dev-master"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "drmonty/leaflet",
        "version": "1.0.0-rc.3",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/mrohnstock/leaflet/archive/1.0.0-rc.3.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/mrohnstock/leaflet.git",
          "type": "git",
          "reference": "1.0.0-rc.3"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "lvoogdt/Leaflet.awesome-markers",
        "version": "2.0.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/lvoogdt/Leaflet.awesome-markers/archive/v2.0.2.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "Leaflet/Leaflet.fullscreen",
        "version": "1.0.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/Leaflet/Leaflet.fullscreen/archive/v1.0.1.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "CliffCloud/Leaflet.EasyButton",
        "version": "1.3.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/CliffCloud/Leaflet.EasyButton/archive/v1.3.1.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "imakewebthings/waypoints",
        "version": "4.0.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/imakewebthings/waypoints/archive/4.0.0.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "socib/Leaflet.TimeDimension",
        "version": "1.0.6",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/socib/Leaflet.TimeDimension/archive/v1.0.6.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "nezasa/iso8601-js-period",
        "version": "0.2.0.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/nezasa/iso8601-js-period/archive/v0.2.0.0.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "StevenLevithan/dateFormat",
        "version": "1.2.3",
        "type": "drupal-library",
        "dist": {
          "url": "https://gist.github.com/eralston/968809/archive/a18b38bede4e3d0e2f1c720bd1e4c010e646bb6d.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "Leaflet/Leaflet.heat",
        "version": "0.2.0",
        "type": "drupal-library",
